{
  "name": "paragonie/ionizer",
  "description": "Powerful input value filtering for PHP applications",
  "authors": [
    {
      "name": "Paragon Initiative Enterprises",
      "email": "info@paragonie.com",
      "homepage": "https://paragonie.com",
      "role": "Development Team"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "ParagonIE\\Ionizer\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParagonIE\\Ionizer\\Test\\": "./tests"
    }
  },
  "license": "ISC",
  "require": {
    "ext-json": "*",
    "php": "^7|^8",
    "paragonie/constant_time_encoding": "^2.1|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5|^7|^8|^9|^10|^11"
  },
  "scripts": {
    "test": ["phpunit", "psalm"]
  },
  "support": {
    "email": "security@paragonie.com",
    "issues": "https://github.com/paragonie/ionizer/issues",
    "source": "https://github.com/paragonie/ionizer"
  }
}